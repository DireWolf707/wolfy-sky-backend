{
  "name": "wolf-shop-backend",
  "private": true,
  "version": "1.0.0",
  "main": "server.js",
  "author": "DireWolf",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": "16.15.1"
  },
  "scripts": {
    "build": "yarn db-gen",
    "start": "node --experimental-specifier-resolution=node src/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "prod": "cross-env NODE_ENV=production yarn start",
    "db-makemigration": "drizzle-kit generate:pg --config src/drizzle/config.js",
    "db-migrate": "cross-env NODE_ENV=development node --experimental-specifier-resolution=node src/drizzle/migrate.js",
    "db-push": "yarn db-makemigration && yarn db-migrate",
    "db-pull": "drizzle-kit introspect:pg --config src/drizzle/config.js"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.18.0",
    "nodemon": "^2.0.22"
  },
  "dependencies": {
    "@google-cloud/storage": "^6.9.5",
    "@sindresorhus/slugify": "^2.2.0",
    "compression": "^1.7.4",
    "connect-redis": "^7.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "drizzle-orm": "^0.26.0",
    "drizzle-zod": "^0.4.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "express-session": "^1.17.3",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "postgres": "^3.3.4",
    "redis": "^4.6.6",
    "socket.io": "^4.6.1",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  }
}
